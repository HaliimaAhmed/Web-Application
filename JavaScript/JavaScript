}		
	</script>

<!-- Insert your exercice 5 javascript between the script tags below-->

	<script>	
var answer = prompt("Please enter a number");

if(isNaN(parseFloat(answer))){
do{
answer = prompt ("You did not enter a number. Please enter a number. ");
}while(isNaN(parseFloat(answer)));
}	
alert(answer + "is a number");
	
</script>

<!-- Insert your exercice 6 javascript between the script tags below-->
	
<script>
	var answer1= parseFloat(prompt("Please enter a number"));
	
if(isNaN(answer1)){
do{
answer1= paraFloat(prompt("You did not enter a number. Please enter a number."));
}while(isNaN(answer1));
}

var answer2= parseFloat(prompt("Please enter a number"));
if(isNaN(answer2)){
do{
answer2= paraFloat(prompt("You did not enter a number. Please enter a number."));
}while(isNaN(answer2));
}		

if(answer1 = answer2){
alert("the number are equal.");
}
if(answer1>answer2){
alert(answer1 + "is greater than" + answer2);
}else{
alert(answer1 + "is less than or equal to " + answer2);
}
	</script>
